<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="#8f8f8f"
	>
	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="match_parent"
		android:layout_height="65dp">
		<ImageView
			android:id="@+id/menu_icon1"
			style="@style/MenuIconIVStyle"
			android:src="@drawable/ic_baseline_nightmode_24"
			/>
		<ImageView
			android:id="@+id/menu_icon2"
			style="@style/MenuIconIVStyle"
			android:src="@drawable/ic_baseline_share_24"
			/>
		<ImageView
			android:id="@+id/menu_icon3"
			style="@style/MenuIconIVStyle"
			android:src="@drawable/ic_baseline_history2_24"
			/>
		<ImageView
			android:id="@+id/menu_icon4"
			style="@style/MenuIconIVStyle"
			android:src="@drawable/star_ic_1"
			/>
		<ImageView
			android:id="@+id/menu_icon5"
			style="@style/MenuIconIVStyle"
			android:src="@drawable/ic_baseline_settings_24"
			/>
	</LinearLayout>

	<LinearLayout
		android:orientation="horizontal"
		android:layout_width="match_parent"
		android:layout_height="65dp">
		<ImageView
			android:id="@+id/menu_icon6"
			style="@style/MenuIconIVStyle"
			android:src="@drawable/ic_baseline_image_24"
			/>
		<ImageView
			android:id="@+id/menu_icon7"
			style="@style/MenuIconIVStyle"
			android:src="@drawable/ic_baseline_chat_bubble_24"
			/>
		<ImageView
			android:id="@+id/menu_icon8"
			style="@style/MenuIconIVStyle"
			android:src="@drawable/ic_baseline_free_breakfast_24"
			/>
		<ImageView
			android:id="@+id/menu_icon9"
			style="@style/MenuIconIVStyle"
			android:src="@drawable/ic_baseline_annot_24"
			/>
		<ImageView
			android:id="@+id/menu_icon10"
			style="@style/MenuIconIVStyle"
			android:src="@drawable/ic_baseline_color_24"
			/>

	</LinearLayout>
</LinearLayout>